package trimestre3;
import java.util.Scanner;

public class Trimestre {

	public static void main(String[] args) {
		
		Scanner leer = new Scanner(System.in);
		System.out.println("Ingrese la tabla de multiplicacion");
		int data = leer.nextInt();
		System.out.println("Hasta que numero de la tabla desea llegar");
		int increment = leer.nextInt();
		int indice = 1;
		do {
			
			System.out.println(data+ " X "+ indice + " =" + (indice * data));
			indice++;
			
		}while(indice <= increment);		
		
	}

}

